function isCheburnet(){try{return window.location.host.includes('aviasales.ru');}catch(e){return false;}}
function escapeCheburnet(result){if(typeof result==='string'){return isCheburnet()?result.replace('aviasales.com','aviasales.ru'):result.replace('aviasales.ru','aviasales.com');}
